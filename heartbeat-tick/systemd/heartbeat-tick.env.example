# Environment configuration for heartbeat-tick systemd service
# Copy this file to /etc/heartbeat-tick.env and update with your values

# Required: RabbitMQ connection
RABBITMQ_URL=amqp://delorenj@localhost:5673/
RABBITMQ_PASS=your-rabbitmq-password-here

# Optional: Exchange configuration (defaults shown)
BLOODBANK_EXCHANGE=bloodbank.events.v1

# Optional: Tick interval in seconds (default: 60)
TICK_INTERVAL_S=60

# Optional: Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
